{
  "name": "manaba-deadline-highlight",
  "version": "0.1",
  "description": "Easily check for upcoming assignments in manaba",
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "lint": "eslint src/**/*.js",
    "lintfix": "eslint --fix src/**/*.js"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "size-plugin": "^2.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  }
}
