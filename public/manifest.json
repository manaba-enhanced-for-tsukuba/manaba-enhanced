{
  "manifest_version": 2,
  "name": "manaba Deadline Highlight",
  "version": "0.1",
  "description": "Easily check for upcoming assignments in manaba",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://manaba.tsukuba.ac.jp/*"
      ],
      "run_at": "document_end",
      "js": [
        "manaba.js"
      ]
    }
  ]
}